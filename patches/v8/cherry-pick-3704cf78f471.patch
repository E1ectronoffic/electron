+++ b/src/heap/mark-compact.cc
@@ -2035,7 +2035,7 @@
     // Instead of calling object.IterateBodyFast(cage_base(), &visitor) here
     // we can shortcut and use the precomputed size value passed to the visitor.
     DCHECK_EQ(object.SizeFromMap(map), size);
